<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- css for pagination below -->
    <link rel="stylesheet" href="assets/vendor/datatables/css/jquery.dataTables.min.css">
    <!-- css for filters below -->
    <link rel="stylesheet" href="assets/vendor/datatables/css/dataTables.bootstrap4.css">
    <link rel="stylesheet" href="assets/vendor/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" href="assets/vendor/fonts/circular-std/style.css">
    <link rel="stylesheet" href="assets/libs/css/style.css">
    <!-- addedthisone -->
    <link rel="stylesheet" href="assets/libs/css/manageInput.css">
    <link rel="stylesheet" href="assets/vendor/fonts/fontawesome/css/fontawesome-all.css">
    <link rel="stylesheet" href="assets/vendor/fonts/flag-icon-css/flag-icon.min.css">
    <link rel="stylesheet" href="assets/vendor/datatables/css/buttons.dataTables.min.css">

    <title>Manage Input</title>
    <link rel="icon" href="scale.png" type="image/x-icon">
</head>

<body>
    <!-- main wrapper -->
    <div class="dashboard-main-wrapper">
        <!-- navbar -->
        <%- include('includes/nav'); %>
        <!-- left sidebar -->
        <%- include('includes/leftSidebar'); %>

        <%- include('includes/scripts'); %>

        <!-- wrapper  -->
        <div class="dashboard-wrapper">
            <div class="dashboard-ecommerce">
                <div class="container-fluid dashboard-content ">
                    <!-- pageheader  -->
                    <div class="row">
                        <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
                            <div class="page-header">
                                <div class="page-breadcrumb">
                                    <nav aria-label="breadcrumb">
                                        <ol class="breadcrumb">
                                            <li class="breadcrumb-item"><a href="#"
                                                    class="breadcrumb-link">Dashboard</a></li>
                                            <li class="breadcrumb-item active" aria-current="page">Manage Input</li>
                                        </ol>
                                    </nav>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- end pageheader  -->

                    <div class="card-body">
                        <h1>Edit Info</h1>
                        <form>
                            <div class="form-group">
                                <label for="officeName" class="col-form-label">Name</label>
                                <input id="officeName" type="text" class="form-control" value="User's Name">
                            </div>
                            <div class="form-group">
                                <label for="officeAddress" class="col-form-label">Address</label>
                                <input id="officeAddress" type="text" class="form-control" value="User's Address">
                            </div>
                            <div class="form-group">
                                <label for="officeCounty" class="col-form-label">County</label>
                                <input id="officeCounty" type="text" class="form-control" value="User's County">
                            </div>
                            <div class="form-group">
                                <label for="officeEmail">Email</label>
                                <input id="officeEmail" type="email" placeholder="name@example.com" class="form-control" value="User's Email">
                            </div>
                            <div class="form-group">
                                <label for="officePhone" class="col-form-label">Phone</label>
                                <input id="officePhone" type="text" class="form-control" value="User's Phone">
                            </div>
                            <div class="form-group">
                                <label for="officeFax" class="col-form-label">Fax</label>
                                <input id="officeFax" type="text" class="form-control" value="User's Fax">
                            </div>
                        </form><br>
                        <a class="btn" style="background-color: rgb(11, 6, 82);">Submit Change</a>
                    </div>
                </div>
            </div>
        </div>
        <!-- footer -->
        <%- include('includes/footer'); %>
        <!-- end footer -->
    </div>
    <!-- end wrapper  -->
    </div>
    <!-- end main wrapper  -->


</body>

</html>